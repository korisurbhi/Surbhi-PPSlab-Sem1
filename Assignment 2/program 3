#include <stdio.h>
#include <conio.h>
void main()
{
    int a,b,c,t;
    printf("Enter the 1st angles of Triangle : ");
    scanf("%d",&a);
    printf("Enter the 2nd angles of Triangle : ");
    scanf("%d",&b);
    printf("Enter the 3rd angles of Triangle : ");
    scanf("%d",&c);
    t=a+b+c;
    if(t==180)
    {
        printf("The Triangle is Valid.");
    }
    else
    {
        printf("Triangle is not Valid");
    }
}